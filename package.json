{
  "name": "astro-pagefind",
  "version": "0.0.0-development",
  "description": "Astro integration for Pagefind static site search",
  "type": "module",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
