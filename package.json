{
  "name": "astro-pagefind",
  "version": "0.0.0-development",
  "description": "Astro integration for Pagefind static site search",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts"
    },
    "./components/Search": {
      "import": "./src/components/Search.astro",
      "types": "./src/components/Search.d.ts"
    }
  },
  "files": [
    "src"
  ],
  "scripts": {
    "astro": "astro",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && tsc --noEmit && astro build",
    "preview": "astro preview",
    "semantic-release": "semantic-release",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shishkin/astro-pagefind.git"
  },
  "homepage": "https://github.com/shishkin/astro-pagefind",
  "keywords": [
    "astro",
    "astro-component",
    "astro-integration",
    "withastro",
    "pagefind",
    "search",
    "ui"
  ],
  "author": "Sergey Shishkin <sergey@shishkin.org>",
  "license": "MIT",
  "peerDependencies": {
    "astro": "^2.0.4"
  },
  "devDependencies": {
    "@astrojs/cloudflare": "6.6.2",
    "@astrojs/netlify": "2.4.0",
    "@astrojs/node": "5.3.0",
    "@astrojs/vercel": "3.7.3",
    "@commitlint/cli": "17.6.6",
    "@commitlint/config-conventional": "17.6.6",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "astro": "2.8.4",
    "husky": "8.0.3",
    "prettier": "3.0.0",
    "semantic-release": "21.0.7",
    "typescript": "5.1.6"
  },
  "dependencies": {
    "@pagefind/default-ui": "^0.12.0",
    "pagefind": "^0.12.0",
    "sirv": "^2.0.3"
  }
}
